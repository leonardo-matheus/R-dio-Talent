RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php?url=$1

# Options -Indexes

# Info.ini Seguro
	<files info.ini>
		order allow,deny
		deny from all
	</files>

# base.ini Seguro
	<files base.ini>
		order allow,deny
		deny from all
	</files>